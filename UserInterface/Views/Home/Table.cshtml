@model UserInterface.Models.MainPageViewModel
@using manager.Entities
@{
    ViewBag.Title = "Table";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Таблица</h2>

<div class="team">
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>№</th>
            <th>Команда</th>
        </tr>
        @foreach (User user in Model.Users)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td class="button"><button>Отправить приглашение</button></td>
            </tr>
        }
    </table>
</div>

